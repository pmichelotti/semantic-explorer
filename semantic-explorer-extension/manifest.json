{
  "manifest_version": 2,

  "name": "Semantic Explorer",
  "description": "Semantic Explorer.",
  "version": "0.1",

  "permissions" : [
  	"tabs",
  	"http://*/*",
  	"http://*/"
  ],

  "browser_action": {
    "default_icon": "img/temp-icon.png",
    "default_title": "Explore"
  },

  "background" : {
  	"scripts" : [ "js/jquery-2.0.2.js", "js/background.js" ],
  	"persistent" : false
  }

}